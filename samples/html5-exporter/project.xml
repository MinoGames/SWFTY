<?xml version="1.0" encoding="utf-8"?>
<project>
    
    <meta title="SWFTY" package="jd.boivin.swfty_exporter" version="1.0.0" company="Jean-Denis Boivin" />
    <app main="Main" path="bin" file="SWFTYExporter" />
    <window background="#000000" allowHighDPI="true" fps="60" width="400" height="300" resizable="false" />
    <window hidden="true" if="cli" />
    
    <haxelib name="openfl" />
    <haxelib name="haxe-zip" />
    <haxelib name="haxe-file-save" if="export" />
    <haxelib name="haxe-file-load" if="export" />
    <haxelib name="format" />
    <haxelib name="binpacking" />
    <haxelib name="swfty" />
    <haxelib name="hxbit" />
    <haxelib name="tweenxcore" />

    <source path="src" />
    <source path="../../submodules/openfl/scripts" />

    <haxedef name="gl_stats" if="debug" />
    <!--<haxedef name="sync" unless="html5" />-->
    <haxedef name="trim" if="export" />
    <haxedef name="use_rtti_doc" unless="html5" if="export" />

    <certificate team-id="FBFX3ZZ2WJ" />
    
    <template path="templates" />

</project>